<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="CustomerRegistrationTaskFlow">
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <method-call id="createCustomer"></method-call>
    <method-call id="commit"></method-call>
    <method-call id="createAddress"></method-call>
    <method-call id="rollback"></method-call>
    <view id="PersonalInfo"></view>
    <view id="AddressInfo"></view>
    <view id="ConfirmationScreen"></view>
    <control-flow-rule id="__1">
      <from-activity-id>*</from-activity-id>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
